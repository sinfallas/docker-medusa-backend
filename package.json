{
  "name": "medusa",
  "version": "1.0.0",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "medusa build",
    "import:pages-sections": "medusa exec ./src/scripts/import-pages-section.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "lint": "biome lint .",
    "format": "biome check --write .",
    "format:check": "biome check .",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "sync:all": "medusa exec ./src/scripts/microservice-products/sync-all.ts",
    "sync:attribute-groups": "medusa exec ./src/scripts/microservice-products/syncers/attribute-groups-syncer.ts",
    "sync:attributes": "medusa exec ./src/scripts/microservice-products/syncers/attributes-syncer.ts",
    "sync:branches": "medusa exec ./src/scripts/microservice-products/syncers/branches-syncer.ts",
    "sync:brands": "medusa exec ./src/scripts/microservice-products/syncers/brands-syncer.ts",
    "sync:classifications": "medusa exec ./src/scripts/microservice-products/syncers/classifications-syncer.ts",
    "sync:cross-selling": "medusa exec ./src/scripts/microservice-products/syncers/cross-selling-syncer.ts",
    "sync:families": "medusa exec ./src/scripts/microservice-products/syncers/families-syncer.ts",
    "sync:images": "medusa exec ./src/scripts/microservice-products/syncers/images-syncer.ts",
    "sync:inventory": "medusa exec ./src/scripts/microservice-products/syncers/inventory-syncer.ts",
    "sync:inventory-only": "medusa exec ./src/scripts/microservice-products/syncers/sync-inventory-only.ts",
    "sync:lines": "medusa exec ./src/scripts/microservice-products/syncers/lines-syncer.ts",
    "sync:prices": "medusa exec ./src/scripts/microservice-products/syncers/prices-syncer.ts",
    "sync:product-options": "medusa exec ./src/scripts/microservice-products/syncers/product-option-syncer.ts",
    "sync:product-variants": "medusa exec ./src/scripts/microservice-products/syncers/product-variant-syncer.ts",
    "sync:products": "medusa exec ./src/scripts/microservice-products/syncers/products-syncer.ts",
    "sync:taxonomies": "medusa exec ./src/scripts/microservice-products/syncers/taxonomies-syncer.ts",
    "sync:categories": "medusa exec ./src/scripts/microservice-products/sync-categories-api.ts",
    "sync:products-api": "medusa exec ./src/scripts/microservice-products/sync-products-api.ts",
    "sync:cross-selling-api": "medusa exec ./src/scripts/microservice-products/sync-cross-selling.ts",
    "sync:relations": "medusa exec ./src/scripts/microservice-products/sync-relations.ts",
    "map:activate-products": "medusa exec ./src/scripts/mappers/map-activate-products.ts",
    "map:products-metadata": "medusa exec ./src/scripts/mappers/map-products-metadata.ts",
    "map:create-shipping-profiles": "medusa exec ./src/scripts/mappers/map-create-shipping-profiles.ts",
    "map:create-shipping-options": "medusa exec ./src/scripts/mappers/map-create-shipping-options.ts",
    "map:create-fulfillment-providers": "medusa exec ./src/scripts/mappers/map-create-fulfillment-providers.ts",
    "analyze:activation": "medusa exec ./src/scripts/mappers/analyze-product-activation.ts",
    "etl:branches": "medusa exec ./src/scripts/etl/sync-branches.ts",
    "etl:taxonomies": "medusa exec ./src/scripts/etl/sync-taxonomies.ts",
    "etl:categories": "medusa exec ./src/scripts/etl/sync-categories.ts",
    "etl:products": "medusa exec ./src/scripts/etl/sync-products.ts",
    "etl:variants": "medusa exec ./src/scripts/etl/sync-product-variant.ts",
    "etl:classifications": "medusa exec ./src/scripts/etl/sync-classifications.ts",
    "etl:prices": "medusa exec ./src/scripts/etl/sync-prices.ts",
    "etl:images": "medusa exec ./src/scripts/etl/sync-images.ts",
    "etl:fulfillment-sets-locations": "medusa exec ./src/scripts/etl/sync-fulfillment-sets-locations.ts",
    "life": "medusa db:migrate && medusa exec ./src/scripts/seed.ts && npx medusa develop",
    "docker:up": "docker compose up --build -d",
    "docker:down": "docker compose -f down"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@editorjs/editorjs": "^2.31.0",
    "@editorjs/header": "^2.8.8",
    "@editorjs/list": "^2.0.9",
    "@medusajs/admin-sdk": "2.10.3",
    "@medusajs/cli": "2.10.3",
    "@medusajs/framework": "2.10.3",
    "@medusajs/medusa": "2.10.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@rokmohar/medusa-plugin-meilisearch": "^1.3.5",
    "@types/pg": "^8.15.6",
    "@types/socket.io": "^3.0.1",
    "awilix": "^8.0.1",
    "date-fns": "^4.1.0",
    "fast-xml-parser": "^4.3.5",
    "pg": "^8.13.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@medusajs/test-utils": "2.10.3",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "lefthook": "^1.13.3",
    "nock": "^14.0.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
